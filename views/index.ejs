<body>
    <!-- Nav -->
    <!-- Image and text -->
    <nav class="navbar navbar-dark bg-dark">
        <a class="navbar-brand" href="#">
            <!-- <img src="/docs/4.5/assets/brand/bootstrap-solid.svg" width="30" height="30" class="d-inline-block align-top" alt="logo" loading="lazy"> -->Some ChatApp
        </a>
        <button class="btn btn-outline-success my-2 my-sm-0" style="color: white; background-color: rgb(3, 148, 23);" onclick="logout()">Logout <%= data.user %></button>
    </nav>
    <!-- Nav -->
    <div class="mainBody">
        <div class="contacts" id="contacts_i">
            <div class="userSearch">
